{
    "name": "frischkraft/craft3-webpack5-tailwind2",
    "description": "Craft CMS 3 Boilerplate with TailwindCSS 2 and Webpack 5",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "project"
    ],
  "require": {
    "chasegiunta/scss": "^1.0",
    "craftcms/cms": "^3.6.10",
    "nystudio107/craft-twigpack": "~1.2",
    "nystudio107/craft-seomatic": "~3.3",
    "pennebaker/craft-architect": "~2.4",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.4"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
